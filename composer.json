{
  "name": "remote-tech/sf-stimulus-turbo-ui-lib",
  "description": "ui components and helpers for symfony applications that use stimulus and turbo frames",
  "minimum-stability": "stable",
  "type": "symfony-bundle",
  "license": "proprietary",
  "authors": [
    {
      "name": "Dorin Penea",
      "email": "dorin.penea@sw.io"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "babdev/pagerfanta-bundle": "^4.4",
    "pagerfanta/doctrine-orm-adapter": "^4.7",
    "pagerfanta/twig": "^4.7",
    "sqids/sqids": "^0.4.1",
    "symfony/stimulus-bundle": "^2.19",
    "symfony/twig-bundle": "7.*|6.*",
    "symfony/ux-turbo": "^2.19",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "autoload": {
    "psr-4": {
      "RemoteTech\\SfStimulusTurboUI\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0-dev"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@npm-install"
    ],
    "post-update-cmd": [
      "@npm-install"
    ],
    "npm-install": [
      "npm install"
    ]
  }
}